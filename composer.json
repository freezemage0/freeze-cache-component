{
    "name": "freeze/file-cache-component",
    "description": "File Cache Component for Freeze Framework",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Freeze",
            "email": "freezemage0@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/cache": "^3.0",
        "freeze/serializer-component": "^0.0.0"
    },
    "autoload": {
        "psr-4": {
            "Freeze\\Component\\Cache\\": "src/"
        }
    },
    "require-dev": {
        "cache/integration-tests": "dev-master",
        "phpunit/phpunit": "^11.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Freeze\\Component\\Cache\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-configuration tests/"
    }
}
